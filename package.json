{
  "name": "@whizzes/linx",
  "version": "v0.1.6-beta",
  "description": "API Client for Linx",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "check": "tsc --notEmit",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whizzes/linx-client.git"
  },
  "keywords": [
    "linx",
    "client",
    "http",
    "api"
  ],
  "author": "Esteban Borai <estebanborai@gmail.com> (https://estebanborai.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whizzes/linx-client/issues"
  },
  "homepage": "https://github.com/whizzes/linx-client#readme",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "esbuild": "^0.15.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "typescript": "^4.8.3"
  }
}
